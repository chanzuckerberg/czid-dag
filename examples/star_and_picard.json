{
    "name": "star_dag",
    "output_dir_s3": "s3://idseq-emr-test/star",
    "targets": {
        "fastqs": [
            "norg_13__nacc_35__uniform_weight_per_organism__hiseq_reads__v10__R1.fastq.gz",
            "norg_13__nacc_35__uniform_weight_per_organism__hiseq_reads__v10__R2.fastq.gz"
        ],
        "validate_input_out": [
            "validate_input_summary.json",
            "valid_input1.fastq",
            "valid_input2.fastq"
        ],
        "star_out": [
            "unmapped1.fastq",
            "unmapped2.fastq",
            "alignments.bam"
        ],
        "insert_size_metrics_out": [
            "picard_insert_metrics.txt",
            "insert_size_histogram.pdf"
        ]
    },
    "steps": [
        {
            "in": [
                "fastqs"
            ],
            "out": "validate_input_out",
            "class": "PipelineStepRunValidateInput",
            "module": "idseq_dag.steps.run_validate_input",
            "additional_files": {},
            "additional_attributes": {
                "truncate_fragments_to": 75000000,
                "file_ext": "fastq"
            }
        },
        {
            "in": [
                "validate_input_out"
            ],
            "out": "star_out",
            "class": "PipelineStepRunStar",
            "module": "idseq_dag.steps.run_star",
            "additional_files": {
                "star_genome": "s3://idseq-database/host_filter/human/2018-02-15-utc-1518652800-unixtime__2018-02-15-utc-1518652800-unixtime/human_STAR_genome.tar"
            },
            "additional_attributes": {
                "output_gene_file": "reads_per_gene.star.tab"
            }
        },
        {
            "in": ["star_out"],
            "out": "insert_size_metrics_out",
            "class": "GenerateInsertSizeMetrics",
            "module": "idseq_dag.steps.generate_insert_size_metrics",
            "additional_files": {},
            "additional_attributes": {}
        }
    ],
    "given_targets": {
        "fastqs": {
            "s3_dir": "s3://idseq-bench/5",
            "count_reads": 1,
            "max_fragments": 75000000
        }
    }
}