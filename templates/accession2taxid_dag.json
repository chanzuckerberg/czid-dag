      "nr_loc.sqlite3.lz4",
      "nr_info.sqlite3.lz4",
      "nr_loc.db.lz4"
    ]
  },
  "steps": [
    {
      "in": ["accession2taxid_input", "nt_input", "nr_input"],
      "out": "accession2taxid_out",
